{
  "name": "Flutter Web Dev",
  "image": "dartlang/dart",
  "features": {
    "ghcr.io/devcontainers-contrib/features/flutter:1": {}
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "Dart-Code.dart-code",
        "Dart-Code.flutter"
      ]
    }
  },
  "postCreateCommand": "flutter config --enable-web && flutter doctor"
}
